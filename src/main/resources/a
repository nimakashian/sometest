	-- we don't know how to generate root <with-no-name> (class Root) :(
    create table ids
    (
    	name varchar(20) not null,
    	val bigint(30) not null,
    	constraint name
    		unique (name)
    );

    create table outbound_messages
    (
    	outbound_message_id bigint auto_increment
    		primary key,
    	message_id bigint null,
    	from_mobile_number varchar(20) null,
    	dest_mobile_number varchar(20) null,
    	message_body varchar(280) null,
    	status varchar(30) null,
    	due_date datetime null,
    	ticket varchar(50) null,
    	creation_date timestamp default CURRENT_TIMESTAMP not null,
    	UDH varchar(30) null,
    	dcs mediumint null
    );

    create index omsgid_ind
    	on outbound_messages (message_id);

    create index status_idx
    	on outbound_messages (status);

    create table outbound_statuses
    (
    	outbound_message_id bigint not null,
    	status varchar(30) not null,
    	constraint outbound_message_id
    		unique (outbound_message_id)
    );

    create index msg_status_status
    	on outbound_statuses (status);

    alter table outbound_statuses
    	add primary key (outbound_message_id);

    create table outgoing_message
    (
    	id bigint auto_increment
    		primary key,
    	from_mobile_number varchar(20) null,
    	dest_mobile_number varchar(20) null,
    	message_body varchar(1000) null,
    	status smallint null,
    	due_date datetime null,
    	creation_date timestamp default CURRENT_TIMESTAMP not null,
    	UDH varchar(30) null,
    	dcs mediumint null,
    	priority int null,
    	source_port mediumint null,
    	dest_port mediumint null
    );

    create index ix_outgoing_message_status
    	on outgoing_message (status);

